# sample-deployment.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: python-app-1
  labels:
    app: python-app-1
  namespace: default
spec:
  replicas: 1
  selector:
    matchLabels:
      app: python-app-1
  template:
    metadata:
      labels:
        app: python-app-1
    spec:
      containers:
      - name: python-app-1
        image: python-app-1:local
        imagePullPolicy: IfNotPresent
        ports:
        - containerPort: 5000
